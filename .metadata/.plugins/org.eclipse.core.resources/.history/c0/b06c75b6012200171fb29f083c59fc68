class MyDataClass():
    '''
    classdocs
    '''
    _my_integer=1

    def __init__(self):
        '''
        Constructor
        '''
    @property
    def my_integer(self):
        return(self._my_integer)
    
    
mydataclass1=MyDataClass()
print(mydataclass1.my_integer())


        