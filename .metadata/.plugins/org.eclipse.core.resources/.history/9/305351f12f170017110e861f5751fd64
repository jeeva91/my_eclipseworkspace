from threading import Thread
from queue import Queue

class Consumer_Thread(Thread):
    '''
    classdocs
    '''


    def __init__(self, consume_Q=Queue(0)):
        '''
        Constructor
        '''
        
        print("inside Consumer constructor")
        self.consume_Q= consume_Q
        Thread.__init__(self)
        
    def run(self):
        '''
        '''
        self.consume() 
        
    
    
    def consume(self):
        while(True):
            if(self.consume_Q.not_empty()):
                data=self.consume_Q.get()
                print("got {} from the queue".format(data))
                
        
    
    